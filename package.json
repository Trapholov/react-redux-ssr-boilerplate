{
  "name": "react-redux-ssr-boilerplate",
  "version": "0.0.1",
  "description": "Example of React, Redux and SSR",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all development-prepare development",
    "development-prepare": "universal-webpack --settings ./webpack/universal-webpack.js prepare",
    "development": "npm-run-all --parallel development-build-client development-build-server development-server",
    "development-build-client": "node ./webpack/development.client.js",
    "development-build-server": "webpack --colors --display-error-details --config ./webpack/development.server.js --watch",
    "development-server": "npm-run-all --parallel production-assets-server development-api-server development-page-server development-web-server",
    "development-api-server": "supervisor -q -n error -w ./src/api-server,./src/common ./src/api-server",
    "development-page-server": "supervisor -q -n error -w ./src/page-server,./src/common,./src/client ./src/page-server",
    "development-web-server": "supervisor -q -n error -w ./src/web-server,./src/common ./src/web-server",
    "production": "npm-run-all production-build-client production-build-server production-server",
    "production-build-client": "webpack --colors --display-error-details --config ./webpack/production.client.js",
    "production-build-server": "webpack --colors --display-error-details --config ./webpack/production.server.js",
    "production-server": "npm-run-all --parallel production-assets-server production-api-server production-page-server production-web-server",
    "production-api-server": "node ./src/api-server",
    "production-assets-server": "node ./src/assets-server",
    "production-page-server": "node ./build/server/server.js",
    "production-web-server": "node ./src/web-server",
    "test": "karma start"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "bluebird": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^4.0.0",
    "http-proxy": "^1.16.2",
    "http-proxy-rules": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-convert": "^1.2.0",
    "koa-cookie": "^1.0.0",
    "koa-cors": "0.0.16",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "pure-render-decorator": "^1.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.5",

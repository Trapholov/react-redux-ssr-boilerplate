    "react-svg-loader": "^1.1.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "resize-observer-polyfill": "^1.4.2",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.2",
    "universal-cookie": "^2.0.8",
    "universal-webpack": "^0.3.11",
    "url-loader": "^0.5.9",
    "uuid": "^3.1.0",
